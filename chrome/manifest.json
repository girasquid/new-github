{
  "name": "New GitHub",
  "version": "0.0.1",
  "description": "Fresh look at GitHub repos",
  "content_scripts": [{
    "css": ["css/new-github.css"],
    "js": ["js/jquery.min.js", "js/geopattern.min.js", "js/new-github.js"],
    "matches": ["https://github.com/*/*"],
    "exclude_globs": ["https://github.com/*/followers/"]
  }],
  "permissions": [
    "storage",
    "https://github.com/*/*"
  ],
  "manifest_version": 2
}
